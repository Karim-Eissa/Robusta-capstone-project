includes:
    - ./vendor/larastan/larastan/extension.neon
parameters:

    paths:
        - app
        - config
        - database
        - routes

    excludePaths:
        - database/**.*

    # The level 9 is the highest level
    level: 9

    parallel:
        maximumNumberOfProcesses: 4

    checkGenericClassInNonGenericObjectType: false